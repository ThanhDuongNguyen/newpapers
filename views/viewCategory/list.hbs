<div class="container-fluid pb-4 pt-4 paddding">
    <div class="container paddding">
        <div class="row mx-0">
            <div class="col-md-8 animate-box fadeInLeft animated-fast" data-animate-effect="fadeInLeft">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">News</div>
                </div>
                {{#each this.listNewspaper}}
                <div class="row pb-4">
                    <div class="col-md-5">
                        <div class="fh5co_hover_news_img">
                            <div class="fh5co_news_img"><a href="/news/{{IDPage}}"><img src="../../public/images/{{ImgAvatar}}" class=" w-100 h-100" alt=""></a></div>
                            <div></div>
                        </div>
                    </div>
                    <div class="col-md-7 animate-box fadeInUp animated-fast">
                        <a href="/news/{{IDPage}}" class="fh5co_magna py-2"> {{Title}} </a> <a href="#" class="fh5co_mini_time py-3"> Thomson Smith -
                        April 18,2016 </a>
                        <div class="fh5co_consectetur"> {{TinyContent}}
                        </div>
                    </div>
                </div>
                {{/each}}
 
            </div>

            <div class="col-md-3 animate-box fadeInRight animated-fast" data-animate-effect="fadeInRight">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">Tags</div>
                </div>
                <div class="clearfix"></div>
                {{#each this.listTag}}
                <div class="fh5co_tags_all d-inline-block">
                    <a href="/Tags/{{IDTags}}" class="fh5co_tagg">{{TagName}}</a>
                </div>
                {{/each}}
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom pt-3 py-2 mb-4">Most Popular</div>
                </div>
                
                {{#each this.listPopular}}
                <div class="row pb-3">
                    <div class="col-5 align-self-center">
                       <a href="/news/{{IDPage}}"><img src="{{ImgAvatar}}" alt="img" class="fh5co_most_trading"></a>
                    </div>
                    <div class="col-7 paddding">
                       <a href="/news/{{IDPage}}"><div class="most_fh5co_treding_font">{{Title}}</div></a>
                        <div class="most_fh5co_treding_font_123"> April 18, 2016</div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
        <div class="row mx-0">
            <div class="col-12 text-center pb-4 pt-4">
                <a href="#" class="btn_mange_pagging"><i class="fa fa-long-arrow-left"></i>&nbsp;&nbsp; Previous</a>
                <a href="#" class="btn_pagging">1</a>
                <a href="#" class="btn_pagging">2</a>
                <a href="#" class="btn_pagging">3</a>
                <a href="#" class="btn_pagging">...</a>
                <a href="#" class="btn_mange_pagging">Next <i class="fa fa-long-arrow-right"></i>&nbsp;&nbsp; </a>
             </div>
        </div>
    </div>
</div>